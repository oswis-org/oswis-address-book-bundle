<div class="row">
    <div class="col-12 mt-5">
        <h3>Aktuální tým</h3>
    </div>
    {% for position in organization.memberAndEmployeePositions|default|filter(p => p.publicOnWeb or true)|default %}
        {% include '@OswisOrgOswisAddressBook/web/parts/organization-person-profile.html.twig' with {'contact': position.person} %}
    {% else %}
        <div class="col-12 my-4">
            <p>Aktuálně nejsou k dispozici žádné kontakty k zobrazení.</p>
        </div>
    {% endfor %}
</div>
